/*public*/

.clearfix {clear: both;}
.main {
  padding: var(--padding_0); 
  margin: 150px auto 0;
  background: var(--bg_fff);
}

.banner {
  position: relative;
  background: var(--bg_fff);
  width: var(--width_100);
  height: var(--height_0); 
  margin: var(--margin_0);
  padding: var(--padding_0);
}
.banner .img {
  display: block;
  width: var(--width_100);
  max-width: var(--width_100);
  text-align: var(--text_center);
  margin: var(--margin_0);
}
.text_img {
  display: block;
  width: auto;
  max-width: var(--width_100);
  text-align: var(--text_center);
  margin: var(--margin_0);
}

.text_left {text-align: var(--text_left);}
.text_center {text-align: var(--text_center);}
.text_right {text-align: var(--text_right);}

.map {height:500px;width: 600px; margin: 0px auto 0;border: 1px solid #ccc;}
.map img {width: auto !important; max-width: initial;}
.map b {color: #d44924;font-size: 15px;}
.map .BMap_center {width: 303px !important;}
.map .iw_poi_content {color: #333;font-size: 12px;}

.crumbs_nav {
  width: var(--width_1200px);
  display: inline-flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: var(--margin_b30);
  padding: var(--padding_b10);
  background: transparent;
  border-bottom: 1px solid var(--color_ccc);
}
.crumbs_nav .left_ul {margin: 0;padding: 0;font-size:0;}
.crumbs_nav .left_ul li {display: inline-table;margin-right: 50px;}
.crumbs_nav .left_ul li a {
  font-size: var(--size_22);
  color: var(--color_333);
  padding: var(--padding_b15);
  border-bottom: 3px solid transparent;
  transition: 0.5s;
}
.crumbs_nav .left_ul .active a, .crumbs_nav .left_ul li:hover a {border-bottom: 3px solid var(--color);}
.crumbs_nav .left_ul li:hover a {color: var(--color);}
.crumbs_nav .crumbs_nav_ul {margin: 0;padding: 0;}
.crumbs_nav .crumbs_nav_ul li {
  color: var(--color_fff);
  font-size: var(--size_12);
  display: inline-table;
  font-family: "FontAwesome";
}
.crumbs_nav .crumbs_nav_ul li + li {color: var(--color);font-weight: 600;font-size: var(--size_13);}
.crumbs_nav .crumbs_nav_ul li + li:before {color: var(--color_333);content: "\f105";padding: 0 5px;}
.crumbs_nav .crumbs_nav_ul li a {color: var(--color_333);font-size: var(--size_13);}

.nav_pagination {margin: var(--margin_t50);text-align:var(--text_center);}
.nav_pagination .pagination {margin: var(--margin_0);font-size: 0;}
.nav_pagination .pagination li {display: inline-block;}
.nav_pagination .pagination li a {
  color: var(--color_666);
  font-size: var(--size_16);
  padding: 5px 10px;
  margin: 0 10px 5px 0; 
  border-radius:0;
  background: #e7e7e7;
  border: 1px solid #e7e7e7;
  transition: 0.5s;
}
.nav_pagination .pagination .active a {
  color: var(--color_fff);
  border-color:var(--color);
  background:var(--color);
  background-image: linear-gradient(90deg, #ff6a00 20%, #ff9d00 70%);
}
.nav_pagination .pagination .active a:hover, 
.nav_pagination .pagination li a:hover {
  color: var(--color_fff);
  border-color:var(--color);
  background:var(--color);
  background-image: linear-gradient(90deg, #ff6a00 20%, #ff9d00 70%);
}

.nav_prev_next {margin:var(--margin_t50);font-size: 0;}
.nav_prev_next .list_prev_next {text-align: center;margin:var(--margin_0);}
.nav_prev_next .list_prev_next li {display: inline-block; margin: 0 30px;}
.nav_prev_next .list_prev_next li a {
  font-size: var(--size_30);
  color: var(--color_666);
  width: 50px;
  height: 50px;
  line-height: 50px;
  background: transparent;
  display: inline-block;
  transition: 0.5s;
}
.nav_prev_next .list_prev_next li a:hover{color: var(--color);}
.nav_prev_next .list_prev_next li .hvr-shutter-in-horizontal:before{background: #fafafa;}

.next_prev_list {margin: var(--margin_t30);padding: var(--padding_t30);border-top: 1px dashed var(--color_ccc);} 
.next_prev {margin: var(--margin_0);padding: var(--padding_0);text-align: var(--text_center);}
.next_prev li {display: inline;position: relative;}
.next_prev li a {
  font-size: var(--size_15);
  color: var(--color_999);
  display: inline-block;
  padding: var(--padding_15);
  background-color: var(--bg_fff);
  border: 1px solid var(--color_999);
  border-radius: 100px;
  transition: 0.5s;
}
.next_prev li a:hover {color: var(--color);border-color: var(--color);}
.next_prev .prev a {float: left;}
.next_prev .next a {float: right;}

.index_title {margin: var(--margin_b20);text-align: var(--text_center);}
.index_title h2 {
  font-weight: 600;
  font-size: var(--size_30);
  color: #525252;
  line-height: 30px;
  margin: var(--margin_b15);
}
.index_title p {
  font-size: var(--size_20);
  color: #959595;
  line-height: 20px;
  margin: var(--margin_0);
}
/*public*/

/*header*/
.header .header_text {
  width: var(--width_100);
  margin: var(--margin_0);
  padding: var(--padding_0);
  background: var(--color);
}
.header_text_content {
  width: var(--width_1200px);
  margin: var(--margin_0);
  padding: 5px 0;
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-template-rows: auto;
  justify-content: center;
  grid-column-gap: 0px;
  grid-row-gap: 0px;
}
.header_text_content .left_text {text-align:var(--text_left);}
.header_text_content .left_text p {font-size: var(--size_15);color:var(--color_fff);}
.header_text_content .left_text p i {margin-right: 10px;}
.header_text_content .right_text {    
  position: relative;
  display: inline-flex;
  justify-content: flex-end;
  flex-wrap: wrap;
}
.header_text_content .right_text .ul {font-size: 0;margin: 0;}
.header_text_content .right_text .ul li {margin-right: 10px;display: inline-block;}
.header_text_content .right_text .ul li .text_img {margin-right: 5px;display: initial;}
.header_text_content .right_text .ul li a {
  font-size: var(--size_15);
  color:var(--color_fff);
  text-transform: uppercase;
  line-height: 20px;
}
.header_text_content .right_text .form_search {margin-left: 30px;}
.header_text_content .right_text .form_search .form_input::-webkit-input-placeholder {
  color: var(--color_999);
  font-size: var(--size_13);
  text-transform: uppercase;
}
.header_text_content .right_text .form_search .form_input {
  color: var(--color_999);
  font-size: var(--size_13);
  padding: 0 10px;
  border: 1px solid transparent;
  transition: 0.5s;
}
.header_text_content .right_text .form_search .form_input:focus {
  outline: 2;
  border: 1px solid var(--color_333);
  -webkit-box-shadow: inset 0 1px 1px rgba(51, 51, 51, 0.3), 0 0 2px rgba(51, 51, 51, 0.3);
  box-shadow: inset 0 1px 1px rgba(51, 51, 51, 0.3), 0 0 2px rgba(51, 51, 51, 0.3);
}
.header_text_content .right_text .form_search .search_btn {
  color: var(--color_999);
  font-size: var(--size_13);
  background: var(--bg_fff);
  padding: 0px 10px;
  border: 1px solid transparent;
  border-left: 1px solid var(--color_999);
  cursor: pointer;
  margin-left: -5px;
}
/*header*/

/*slider*/
.slider .item {
  background-size:cover;
  background-position:center center;
  display:-webkit-box;
  display:-ms-flexbox;
  display:-webkit-flex;
  display:flex;
  -webkit-box-pack:center;
  -ms-flex-pack:center;
  -webkit-justify-content:center;
  justify-content:center;
  -webkit-box-orient:vertical;
  -webkit-box-direction:normal;
  -ms-flex-direction:column;
  -webkit-flex-direction:column;
  flex-direction:column;
  position:relative;
}
.slider .owl-dots .owl-dot.active {background-color:#cf4901;}
.slider .owl-dots .owl-dot {
  -webkit-transition:all 0.3s cubic-bezier(0.84,0.35,0.39,0.74) 0s;
  transition:all 0.3s cubic-bezier(0.84,0.35,0.39,0.74) 0s;
}
.slider .owl-dots {
  display:inline-block;
  position:absolute;
  left:50%;
  bottom:20px;
  -webkit-transform:translateX(-50%);
  transform:translateX(-50%);
}
.slider .owl-dots .owl-dot {
  background-color: #fff;
  display:inline-block;
  width: 20px;
  height: 8px;
  margin: 0 3px;
  border-radius: 1px;
}
.slider .owl-dots .owl-dot.active {width:40px;}
.slider .owl-nav {
  position: absolute;
  left: 0;
  right: 0;
  top: 50%;
  bottom: auto;
  width: 100%;
  text-align: center;
  line-height: 30px;
}
.slider .owl-nav button.owl-prev, .slider .owl-nav button.owl-next {
  position: absolute;
  top: 0;
  bottom: auto;
  height: 50px;
  width: 50px;
  text-align: center;
  line-height: 30px;
  font-size: 30px;
  color: #fff;
  border-radius: 100%;
  border: 2px solid #fff;
  opacity: 0.5;
  transition: 0.5s;
}
.slider .owl-nav [class*='owl-']:hover {background: transparent;}
.slider .owl-nav button.owl-prev:hover, .slider .owl-nav button.owl-next:hover {opacity: 1;}
.slider .owl-nav .owl-prev {left:0;right:auto;}
.slider .owl-nav .owl-next {left:auto;right:0;}
.slider .owl-dots .owl-dot span {display: none;}
/*slider*/

/*index_trained*/
.index_trained {
  width: var(--width_100);
  padding: var(--padding_0);
  margin: var(--margin_50);
  background: var(--bg_fff);
}
.index_trained .index_trained_content {
  width: var(--width_1200px);
  padding: var(--padding_0);
  margin: var(--margin_0);
}
.index_trained .index_trained_content .h5 {
  text-align: var(--text_center);
  font-weight: 600;
  font-size: var(--size_16);
  color: var(--color);
  margin: var(--margin_0);
  padding: 30px 0;
  background: #eee;
}
.index_trained_content .index_trained_list {margin: var(--margin_t30);}
.index_trained_content .index_trained_list .figure_img .figcaption_text {
  padding: var(--padding_t10);
  margin: var(--margin_0);
}
.index_trained_content .index_trained_list .figure_img {margin: 0 15px;}
.index_trained_content .index_trained_list .a {position: relative;display: block;overflow: hidden;}
.index_trained_content .index_trained_list .a:before, 
.index_trained_content .index_trained_list .a:after {
  position: absolute;
  content: "";
  height: 100%;
  width: 100%;
  z-index: 8;
  -webkit-transition: all 0.5s ease 0s;
  transition: all 0.5s ease 0s;
  background: rgba(6, 6, 6, 0.05);
}
.index_trained_content .index_trained_list .a:before {left: -100%;top: -100%;}
.index_trained_content .index_trained_list .a:after {bottom: -100%;right: -100%;}
.index_trained_content .index_trained_list .a:hover:before {left: 0;top: 0;}
.index_trained_content .index_trained_list .a:hover:after {bottom: 0;right: 0;}
.index_trained_content .index_trained_list .figure_img .figcaption_text h3 {
  position: relative;
  line-height: 20px;
  margin: var(--margin_0);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.index_trained_content .index_trained_list .figure_img .figcaption_text h3 a {
  color: var(--color_333);
  font-size: var(--size_16);
  transition: 0.5s;
}
.index_trained_content .index_trained_list .figure_img:hover .figcaption_text h3 a {color: var(--color);}
.index_trained_content .index_trained_list .figure_img .figcaption_text span {
  font-size: var(--size_12);
  color: var(--color_666);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.index_trained_list .owl-dots .owl-dot span {
  width: 10px;
  height: 10px;
  margin: 0px 5px;
  background: var(--color_ccc);
  border-radius: 50px;
  display: block;
  transition: 0.5s;
}
.index_trained_list .owl-dots .owl-dot.active span, 
.index_trained_list .owl-dots .owl-dot:hover span {background: var(--color);}
/*index_trained*/

