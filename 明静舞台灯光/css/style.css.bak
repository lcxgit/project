.trained_content .text .link {
  position: relative;
  padding: 5px 20px;
  font-size: var(--size_15);
  font-weight: 500;
  letter-spacing: 1px;
  background-color: #ff5900;
  border-radius: 100px;
  border: 1px solid var(--color);
  overflow: hidden;
  position: absolute;
  right: 0;
  bottom: 10px;
}
.trained_content .text .link::before {background:var(--bg_fff);}
.trained_content .text .left_text {width: 550px;margin: 0 0 0 0;}
.trained_content .text .left_text .figure_img {margin: var(--margin_t50);}
.trained_content .text .left_text .figure_img .figure_img_a img {width: 100%;}
.trained_content .text .left_text .figure_img .figcaption_text {padding: 20px 0 0 20px;position: relative;}
.trained_content .text .left_text .figure_img .figcaption_text h3 {
  position: relative;
  line-height: 20px;
  margin: var(--margin_b10);
}
.trained_content .text .left_text .figure_img .figcaption_text h3 a::before {
  background: var(--color);
  position: absolute;
  top: auto;
  bottom: -10px;
  left: 0;
  right: auto;
}
.trained_content .text .left_text .figure_img .figcaption_text h3 a:hover::before {width: 100%;}
.trained_content .text .left_text .figure_img .figcaption_text h3 a {
  position: relative;
  color: var(--color_333);
  font-size: var(--size_16);
  font-weight: 600;
  transition: 0.5s;
}
.trained_content .text .left_text .figure_img:hover .figcaption_text h3 a {color: var(--color);}
.trained_content .text .left_text .figure_img .figcaption_text span {
  color: #ff5900;
  font-size: var(--size_14);
  font-weight: 600;
}
.trained_content .text .right_text {width: 550px;margin: 0 0 0 0;}
.trained_content .text .right_text .figure_img {margin: var(--margin_t50);}
.trained_content .text .right_text .figure_img .figure_img_a {position: relative;display: block;}
.trained_content .text .right_text .figure_img .figure_img_a .text_img {width: 100%;}
.trained_content .text .right_text .figure_img .figcaption_text {padding: 20px 0 0 20px;position: relative;}
.trained_content .text .right_text .figure_img .figcaption_text h3 {line-height: 20px;margin: var(--margin_b10);}
.trained_content .text .right_text .figure_img .figcaption_text h3 a {
  position: relative;
  color: var(--color_333);
  font-size: var(--size_16);
  line-height: 20px;
  font-weight: 600;
  transition: 0.5s;
}
.trained_content .text .right_text .figure_img:hover .figcaption_text h3 a {color: var(--color);}
.trained_content .text .right_text .figure_img .figcaption_text h3 a::before {
  background: var(--color);
  position: absolute;
  top: auto;
  bottom: -10px;
  left: 0;
  right: auto;
}
.trained_content .text .right_text .figure_img .figcaption_text h3 a:hover::before {width: 100%;}
.trained_content .text .right_text .figure_img .figcaption_text span {
  color: #ff5900;
  font-size: var(--size_14);
  font-weight: 600;
}
.trained_content .text .right_text .product_detail_title {margin: 200px auto 50px;text-align: var(--text_right);}
.trained_content .text .right_text .product_detail_title span {
  position: relative;
  color: var(--color_333);
  font-size: 40px;
  text-transform: uppercase;
}
.trained_content .text .right_text .product_detail_title h3 {
  position: relative;
  z-index: 1;
  font-size: 36px;
  color:var(--color_333);
  margin: var(--margin_t10);
}
.trained_content .text .right_text .product_detail_title h3::after {
  content: "";
  position: absolute;
  z-index: -1;
  top: auto;
  bottom: 0;
  left: auto;
  right: 0px;
  width: 110px;
  height: 15px;
  background: #e60012;
  margin: var(--margin_0);
}
/*trained*/

/*news*/
.news {
  width: var(--width_100);
  padding: var(--padding_0);
  margin: var(--margin_0);
  background: var(--bg_fff);
}
.news .news_content {
  width: var(--width_1200px);
  padding: var(--padding_0);
  margin: var(--margin_50);
}
.news_content .news_list {margin: var(--margin_0);}
.news_content .news_list .figure_img {
  position: relative;
  display: inline-flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin: var(--margin_b30);
  font-size: 0;
}
.news_content .news_list .figure_img .figure_img_a {
  position: relative;
  overflow: hidden;
  width: 480px;
}
.news_content .news_list .figure_img .figure_img_a:before, 
.news_content .news_list .figure_img .figure_img_a:after {
  position: absolute;
  content: "";
  height: 100%;
  width: 100%;
  z-index: 8;
  -webkit-transition: all 0.5s ease 0s;
  transition: all 0.5s ease 0s;
  background: rgba(6, 6, 6, 0.05);
}
.news_content .news_list .figure_img .figure_img_a:before {left: -100%;top: -100%;}
.news_content .news_list .figure_img .figure_img_a:after {bottom: -100%;right: -100%;}
.news_content .news_list .figure_img:hover .figure_img_a:before {left: 0;top: 0;}
.news_content .news_list .figure_img:hover .figure_img_a:after {bottom: 0;right: 0;}

.news_content .news_list .figure_img .figcaption_text {width: 690px;margin-left: 30px;}
.news_content .news_list .figure_img .figcaption_text a {display: block;}
.news_content .news_list .figure_img .figcaption_text h3 {
  position: relative;
  display: inline-table;
  font-size: var(--size_16);
  color: var(--color_333);
  font-weight:600;
  margin: var(--margin_b15);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  -webkit-transition: all 0.6s ease 0s;
  transition: all 0.6s ease 0s;
}
.news_content .news_list .figure_img:hover .figcaption_text h3 {color: var(--color);padding-left: 15px;}
.news_content .news_list .figure_img .figcaption_text h3::before {
  background: var(--color);
  position: absolute;
  top: auto;
  bottom: -5px;
  left: 0;
  right: auto;
  -webkit-transition: all 0.6s ease 0s;
  transition: all 0.6s ease 0s;
}
.news_content .news_list .figure_img:hover .figcaption_text h3::before {width: 96%;left: 15px;}
.news_content .news_list .figure_img .figcaption_text p {
  font-size: var(--size_14);
  color: var(--color_999);
  margin: var(--margin_b30);
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: var(--p_text_4);
  text-overflow: ellipsis;
  word-break: break-all;
  height: 100px;
}
.news_content .news_list .figure_img .figcaption_text span {
  font-size: var(--size_30);
  color: var(--color_999);
  margin-right: 10px;
}
.news_content .news_list .figure_img .figcaption_text small {
  font-size: var(--size_14);
  color: var(--color_999);
}
.news_content .news_list .article_text {
  padding: var(--padding_10);
  margin: var(--margin_b30);
  box-shadow: 2px 2px 25px rgba(0, 0, 0, 0.15);
}
.news_content .news_list .article_text .a {display: block;}
.news_content .news_list .article_text h3 {
  position: relative;
  display: inline-table;
  font-size: var(--size_16);
  color: var(--color_333);
  font-weight:600;
  margin: var(--margin_b15);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  transition: 0.5s;
}
.news_content .news_list .article_text:hover h3 {color: var(--color);}
.news_content .news_list .article_text h3::before {
  background: var(--color);
  position: absolute;
  top: auto;
  bottom: -5px;
  left: 0;
  right: auto;
}
.news_content .news_list .article_text h3:hover::before {width: 100%;}
.news_content .news_list .article_text p {
  font-size: var(--size_14);
  color: var(--color_999);
  margin: var(--margin_b5);
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: var(--p_text_2);
  text-overflow: ellipsis;
  word-break: break-all;
  height: 55px;
}
.news_content .news_list .article_text small {
  font-size: var(--size_14);
  color: var(--color_999);
  float: right;
}
/*news*/

/*news_detail*/
.news_detail {
  width: var(--width_100);
  padding: var(--padding_0);
  margin: var(--margin_0);
  background: var(--bg_fff);
}
.news_detail .news_detail_content {
  width: var(--width_1200px);
  padding: var(--padding_0);
  margin: var(--margin_50);
}
.news_detail_content .title_text {
  margin: var(--margin_b20);
  padding: var(--padding_b15);
  text-align: var(--text_left);
  border-bottom: 1px dashed var(--color_ccc);
}
.news_detail_content .title_text h2 {
  font-size: var(--size_25);
  color: var(--color_666);
  font-weight:600;
  margin: var(--margin_b15);
}
.news_detail_content .title_text ul {margin: var(--margin_0);padding: var(--padding_0);}
.news_detail_content .title_text ul li {
  color: var(--color_333);
  font-size: var(--size_12);
  padding: 0 20px 0 0;
  display: inline-block;
}
.news_detail_content .text .p {color: var(--color_666);font-size: var(--size_14);text-indent: 30px;}
.news_detail_content .text .span {color: var(--color_666);font-size: var(--size_14);}
/*news_detail*/

/*contact*/
.contact {
  width: var(--width_100);
  padding: var(--padding_0);
  margin: var(--margin_0);
  background: var(--bg_fff);
}
.contact_content {
  width: var(--width_1200px);
  padding: var(--padding_0);
  margin: var(--margin_50);
}
.contact_content .left_text {float: left;width: 550px;margin-right: 50px;}
.contact_content .text .h2 {
  color: var(--color);
  font-size: 35px;
  font-weight: 500;
  margin: var(--margin_b15);
}
.contact_content .text .span {color: var(--color_999);font-size: var(--size_20);text-transform:uppercase;}
.contact_content .text .p {
  color: var(--color_666);
  font-size: var(--size_16);
  margin: var(--margin_t20);
}
/*contact*/

@media (max-width: 1230px) {
  :root{
    --width_1200px: 1170px;
  }
  .index_product_content .index_product_list .figure_img {margin: 0 0px 10px;}
  .index_uthenticity_content .text .text_img:nth-child(1) {margin: -100px 55px 0 0;}
  .index_certificate_list .owl-nav .owl-prev {right: auto;left: 0px;}
  .index_certificate_list .owl-nav .owl-next {right: 0px;left: auto;}
  .index_about_content .text .div_flex .p {width: 590px;}
  .contact_content .left_text {margin-right: 20px;}
  .product_content .product_list .figure_img {width: 530px;}
  .news_content .news_list .figure_img .figcaption_text {width: 660px;}
  .product_detail_content .product_detail_list {grid-column-gap: 40px;}
  .product_detail_content .right_text {width: 690px;}
  .footer_content .footer_nav_list {grid-template-columns: 110px 110px 110px 110px 110px 110px 110px 410px;}
}

